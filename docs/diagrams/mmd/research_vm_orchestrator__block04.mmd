    TEXT session_id PK
    TEXT user_id
    TEXT query
    TIMESTAMP created_at
    TEXT status
    JSON metadata
  }
  research_sources {
    TEXT source_id PK
    TEXT session_id
    TEXT url
    TEXT title
    TEXT content
    TIMESTAMP extracted_at
    REAL quality_score
    REAL credibility_score
    REAL bias_score
    JSON metadata
  }
